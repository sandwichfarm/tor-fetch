{
  "name": "@sandwichfarm/torfetch",
  "version": "0.1.0",
  "type": "module",
  "description": "TorFetch: Fetch API over the Tor network.",

  "exports": {
    ".": {
      "require": "./dist/server/index.cjs.js",
      "import": "./dist/server/index.js",
      "default": "./dist/server/index.js",
      "types": "./dist/server/types/index.d.ts"
