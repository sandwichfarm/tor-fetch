    "@rollup/plugin-typescript": "^11.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-polyfill-node": "0.13.0",
    "rollup-plugin-terser": "7.0.2",
    "tsc-alias": "1.8.10",
    "tslib": "2.7.0",
    "typescript": "^5.0.0",
